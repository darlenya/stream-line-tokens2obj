{
  "name": "kronos-interceptor-line-tokens2obj",
  "version": "0.0.0-semantic-release",
  "description": "Splits a line by a sparator into tokens.",
  "main": "dist/module.js",
  "module": "src/module.js",
  "scripts": {
    "cover": "nyc npm test",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/module.js -c -- src/module.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-line-tokens2obj.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-line-tokens2obj/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor-line-tokens2obj#readme",
  "dependencies": {
    "kronos-interceptor": "^2.4.11",
    "z-schema": "3.18.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "kronos-test-interceptor": "2.1.0",
    "mocha": "^3.2.0",
    "semantic-release": "^6.3.6",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.41.4",
    "nyc": "^10.1.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.4"
  },
  "keywords": [
    "kronos-interceptor",
    "stream",
    "line",
    "parser"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  }
}